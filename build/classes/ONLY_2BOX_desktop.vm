<section class="ONLY_2BOX" $header>
   <div class='row'>
      <div class='col-md-6'>
         <h1>$slide.getTitle().getText()</h1>
         <ul>
            #if($slide.getList().getItems().size()>6)
            #if(!$slide.getList().getItems().get(6).getText().isEmpty())
            <div class="fragment fade-up " data-fragment-index="6">$slide.getList().getItems().get(6).getText()</div>
             #end
             #end
              #if($slide.getList().getItems().size()>0)
            #if(!$slide.getList().getItems().get(0).getText().isEmpty())
            <div class="fragment fade-up " data-fragment-index="0">$slide.getList().getItems().get(0).getText()</div>
             #end
             #end
         </ul>
      </div>
      <div class='col-md-6'>
         <h1 >$slide.getTitle2().getText()</h1>
         <ul>
            #if($slide.getList().getItems().size()>7)
             #if(!$slide.getList().getItems().get(7).getText().isEmpty())
            <div class="fragment fade-up " data-fragment-index="7">$slide.getList().getItems().get(7).getText()</div>
           #end
           #end
            #if($slide.getList().getItems().size()>1)
             #if(!$slide.getList().getItems().get(1).getText().isEmpty())
            <div class="fragment fade-up " data-fragment-index="1">$slide.getList().getItems().get(1).getText()</div>
           #end
           #end
         </ul>
      </div>
   </div>
</section>